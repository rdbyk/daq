  <LANGUAGE>eng</LANGUAGE>
  <TITLE>DAQ Constants</TITLE>
  <TYPE>DAQ Toolbox Functions</TYPE>
  <DATE>20-Dec-2007</DATE>
  <SHORT_DESCRIPTION name="DAQ_Constants">DAQ</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>[ value ] = DAQ ( name )</CALLING_SEQUENCE_ITEM> 
  </CALLING_SEQUENCE>

  <PARAM>
  <PARAM_INDENT>
    
    <PARAM_ITEM>
    <PARAM_NAME>name</PARAM_NAME>
    <PARAM_DESCRIPTION>
       <SP>
       : string
       </SP>
    </PARAM_DESCRIPTION>
    </PARAM_ITEM>
    
    <PARAM_ITEM>
    <PARAM_NAME>value</PARAM_NAME>
    <PARAM_DESCRIPTION>
       <SP>
       : integer
       </SP>
    </PARAM_DESCRIPTION>
    </PARAM_ITEM>
    
  </PARAM_INDENT>
  </PARAM>
 
  <DESCRIPTION>
     <DESCRIPTION_INDENT>
     <DESCRIPTION_ITEM>
     <P>
     <BD>DAQ("NAME")</BD> returns the value of a predefined constant
     <BD>DAQmx_NAME</BD>.
     </P>
     </DESCRIPTION_ITEM>
     </DESCRIPTION_INDENT>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[
   // value of DAQmx_Val_GroupByChannel
   value = DAQ("Val_GroupByChannel");
  ]]></EXAMPLE>

